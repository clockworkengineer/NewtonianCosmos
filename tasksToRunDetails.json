[{
        "taskName": "File Copier",
        "watchFolder": "watch",
        "processDetails": {
            "prog": "node",
            "args": ["./FPE_copyFiles.js", "destination"]
         },
         "runTask": true
   }, {
        "taskName": "Video File Conversion",
        "watchFolder": "WatchFolderVideos",
        "processDetails": {
            "prog": "node",
            "args": ["./FPE_handbrake.js", "./destinationConverted", "{ \".mkv\" : true, \".avi\" : true, \".mp4\" : true}"]
        },
        "runTask": true
   }, {
        "taskName": "File ePrinter",
        "watchFolder": "ePrintWatch",
        "processDetails": {
            "prog": "node",
            "args": ["./FPE_eprint.js", "{ \".docx\" : true, \".rtf\" : true, \".txt\" : true}"]
        },
        "runTask": true
 }, {
        "taskName": "File Copier On Extension",
        "watchFolder": "watch",
        "processDetails": {
            "prog": "node",
            "args": ["./FPE_copyFilesOnExt.js", "destination", "{ \".docx\" : \"documents\" , \".zip\" : \"zipped\" }"]
        },
        "runTask": true
    }]
